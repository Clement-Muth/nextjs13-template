{
  "name": "nextjs13-template",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "app/",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "compile": "yarn workspace @nextjs13-template/app tsc -p .",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "start": "yarn workspace @nextjs13-template/app start",
    "test": "turbo run test"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.2.0",
    "@next/font": "^13.0.6",
    "lint": "^0.7.0",
    "next": "^13.0.6",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rebass": "^4.0.7",
    "sharp": "^0.31.1",
    "swr": "^2.0.0",
    "tslint": "^6.1.3",
    "turbo": "^1.6.3"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@types/next": "^9.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/rebass": "^4.0.10",
    "cross-env": "^7.0.3",
    "node-typescript-compiler": "^3.0.0",
    "nprogress": "^0.2.0",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@1.22.17"
